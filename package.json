{
  "name": "my-finance-react",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "ts-node generate.ts && react-scripts build",
    "test": "react-scripts test",
    "plop": "plop"
  },
  "dependencies": {
    "@material-design-icons/svg": "^0.10.7",
    "@material/button": "^13.0.0",
    "@material/data-table": "^13.0.0",
    "@material/dialog": "^13.0.0",
    "@material/ripple": "^13.0.0",
    "@ngneat/falso": "^5.3.0",
    "@rmwc/button": "^7.0.0",
    "@rmwc/checkbox": "^7.0.0",
    "@rmwc/data-table": "^7.0.0",
    "@rmwc/dialog": "^7.0.0",
    "@rmwc/formfield": "^7.0.0",
    "@rmwc/icon": "^7.0.0",
    "@rmwc/textfield": "^7.0.0",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.20",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-table": "^7.7.9",
    "classnames": "^2.3.1",
    "cyrillic-to-translit-js": "^3.1.0",
    "dayjs": "^1.10.7",
    "downshift": "^6.1.7",
    "fs-extra": "^10.1.0",
    "just-percentile": "^4.0.0",
    "lodash": "^4.17.21",
    "material-design-icons": "^3.0.1",
    "mobx": "^6.4.1",
    "mobx-react-lite": "^3.3.0",
    "object-hash": "^3.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "react-table": "^7.7.0",
    "react-toastify": "^8.2.0",
    "rumble-charts": "^4.0.0",
    "sass": "^1.49.8",
    "web-vitals": "^2.1.4"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@kreozot/eslint-config"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "rules": {
      "react/jsx-props-no-spreading": "off",
      "@typescript-eslint/no-unused-vars": "warn",
      "no-debugger": "warn",
      "@typescript-eslint/indent": [
        "error",
        2,
        {
          "ignoredNodes": [
            "TSTypeParameterInstantiation",
            "TSIntersectionType"
          ]
        }
      ],
      "class-methods-use-this": "off"
    },
    "overrides": [
      {
        "files": [
          "plopfile.ts"
        ],
        "env": {
          "node": true,
          "browser": false
        },
        "rules": {
          "import/no-extraneous-dependencies": "off",
          "no-console": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@kreozot/eslint-config": "^1.0.0",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.178",
    "@types/object-hash": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.9.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "inquirer-directory": "^2.2.0",
    "plop": "^3.0.5",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  }
}
